echo "0118 - ZPT"

. ../tests/common.sh

# Matrices

for f in 2 5 9 25 67 125 256 ; do
   zpt -Q -r 2 -c 3 tmp.$$.x$f \
      "${MTX_TEST_DATA_DIR}/Mat$f" - "${MTX_TEST_DATA_DIR}/Mat$f" \
      - "${MTX_TEST_DATA_DIR}/Mat$f" -
   compareBinaryWithReference x$f
done

# Permutations

zpt tmp.$$.x "${MTX_TEST_DATA_DIR}/Perm1" "${MTX_TEST_DATA_DIR}/Perm2"
zpr -G tmp.$$.x tmp.$$.x.txt
compareWithReference x.txt
zpt tmp.$$.y tmp.$$.x "${MTX_TEST_DATA_DIR}/Perm1"
zpr -G tmp.$$.y tmp.$$.y.txt
compareWithReference y.txt

rm -f tmp.$$.*
