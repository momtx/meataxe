echo "0110 - ZMO/ZKD"

. ../tests/common.sh

cp "${MTX_TEST_DATA_DIR}/Perm1" tmp.$$.x.1
cp "${MTX_TEST_DATA_DIR}/Perm2" tmp.$$.y.1
zmo -Q -g 1 tmp.$$.x tmp.$$.xorb
zmo -Q -g 1 tmp.$$.y tmp.$$.yorb

zkd -Q 2 tmp.$$.xorb tmp.$$.y.1 tmp.$$.yk2
zkd -Q 2 tmp.$$.yorb tmp.$$.x.1 tmp.$$.xk2

zkd -Q 67 tmp.$$.xorb tmp.$$.y.1 tmp.$$.yk67
zkd -Q 67 tmp.$$.yorb tmp.$$.x.1 tmp.$$.xk67

for f in xk2 xk67 yk2 yk67; do
   zpr tmp.$$.$f tmp.$$.$f.txt
   cmp tmp.$$.$f.txt "${MTX_TESTCASE_DIR}/$f.txt.expected"
done

# Clean up
rm -f tmp.$$.*
