# Test ZSY

. ../tests/common.sh

zsy -Q e2 ../tests/data/Perm1 x1
zsy -Q e3 ../tests/data/Perm1 x2
checksum x1 772.2115894062  || exit 1
checksum x2 4572.2159097617 || exit 1


zct 1-5:1-7 ../tests/data/Mat67 m67
zct 1-5:1-6 ../tests/data/Mat25 m25
zct 1-7:1-7 ../tests/data/Mat9 m9
zsy -Q e2 ../tests/data/Mat5 x1
zsy -Q e3 m67 x2
zsy -Q e4 m25 x3
zsy -Q s2 ../tests/data/Mat2 x4
checksum x1 25404.3020209772 || exit 1
checksum x2 362.1554181026 || exit 1
checksum x3 87.4019966339  || exit 1
checksum x4 11412.1035840264 || exit 1

# Clean up
# --------
rm -f x1 x2 x3 x4 m9 m67 m25

